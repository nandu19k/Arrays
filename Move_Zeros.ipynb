      },
      "source": [
        "def moveZeros(lst):\n",
        "  j = 0\n",
        "  for num in lst:\n",
        "    if num !=0:\n",
        "      lst[j] = num\n",
        "      j+=1\n",
        "  for x in range(j , len(lst)):\n",
        "    lst[x] = 0\n",
        "  return lst"
      ],
      "execution_count": 1,
      "outputs": []
    },
